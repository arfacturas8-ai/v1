# CRYB Mobile App - Staging Environment Configuration

# API Configuration
EXPO_PUBLIC_API_BASE_URL=https://api-staging.cryb.app
EXPO_PUBLIC_WS_URL=wss://api-staging.cryb.app
EXPO_PUBLIC_ENVIRONMENT=staging

# App Configuration
EXPO_PUBLIC_APP_NAME=CRYB (Staging)
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_DEBUG=true

# Feature Flags
EXPO_PUBLIC_ENABLE_BIOMETRICS=true
EXPO_PUBLIC_ENABLE_VOICE_CHAT=true
EXPO_PUBLIC_ENABLE_VIDEO_CHAT=true
EXPO_PUBLIC_ENABLE_SCREEN_SHARE=true
EXPO_PUBLIC_ENABLE_FILE_UPLOAD=true
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true
EXPO_PUBLIC_ENABLE_WEB3=true
EXPO_PUBLIC_ENABLE_ANALYTICS=true

# Third-party Services (Staging)
EXPO_PUBLIC_WALLETCONNECT_PROJECT_ID=your-walletconnect-staging-id
EXPO_PUBLIC_SENTRY_DSN=your-sentry-staging-dsn

# Build Configuration
EXPO_PUBLIC_BUILD_TYPE=staging
EXPO_PUBLIC_LOG_LEVEL=info

# Firebase Configuration (Staging)
EXPO_PUBLIC_FIREBASE_API_KEY=your-firebase-staging-api-key
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=cryb-staging.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=cryb-staging
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=cryb-staging.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
EXPO_PUBLIC_FIREBASE_APP_ID=your-firebase-staging-app-id

# Staging URLs
EXPO_PUBLIC_WEB_URL=https://staging.cryb.app
EXPO_PUBLIC_ADMIN_URL=https://admin-staging.cryb.app

# Security Configuration
EXPO_PUBLIC_ENABLE_SSL_PINNING=false
EXPO_PUBLIC_API_TIMEOUT=15000
EXPO_PUBLIC_MAX_RETRY_ATTEMPTS=5