    <key>NSContactsUsageDescription</key>
    <string>CRYB can optionally access your contacts to help you find friends who are already using the app.</string>
    <key>NSExceptionDomains</key>
    <dict>
      <key>localhost</key>
      <dict>
        <key>NSExceptionAllowsInsecureHTTPLoads</key>
        <true/>
