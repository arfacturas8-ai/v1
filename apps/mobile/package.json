{
  "name": "@cryb/mobile",
  "version": "0.0.1",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:e2e": "detox test",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "expo r -c",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "build:development": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:production": "eas build --profile production",
    "build:ios": "eas build --platform ios --profile production",
    "build:android": "eas build --platform android --profile production",
    "submit:ios": "eas submit --platform ios --profile production",
    "submit:android": "eas submit --platform android --profile production",
    "update": "eas update --branch production",
    "update:preview": "eas update --branch preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@livekit/react-native": "^2.9.1",
    "@livekit/react-native-webrtc": "^137.0.2",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@sentry/react-native": "~5.24.3",
    "@tanstack/react-query": "^5.64.2",
    "eventemitter3": "^5.0.1",
    "expo": "~51.0.0",
    "expo-application": "~5.9.1",
    "expo-av": "^16.0.7",
    "expo-battery": "~8.0.1",
    "expo-camera": "~15.0.16",
    "expo-constants": "~16.0.2",
    "expo-device": "~6.0.2",
    "expo-document-picker": "^12.0.2",
    "expo-file-system": "~17.0.1",
    "expo-haptics": "~13.0.1",
    "expo-image": "~1.13.0",
    "expo-image-manipulator": "^12.0.5",
    "expo-image-picker": "~15.1.0",
    "expo-keep-awake": "~13.0.2",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-local-authentication": "~14.0.1",
    "expo-location": "~17.0.1",
    "expo-media-library": "~16.0.5",
    "expo-network": "~6.0.1",
    "expo-notifications": "~0.28.19",
    "expo-secure-store": "~13.0.2",
    "expo-splash-screen": "~0.27.7",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "~0.25.28",
    "expo-web-browser": "~13.0.3",
    "lucide-react-native": "^0.555.0",
    "react": "18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-native": "0.74.5",
    "react-native-exception-handler": "^2.10.10",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "~2.16.2",
    "react-native-mmkv": "^3.2.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-restart": "^0.0.27",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "^4.17.1",
    "react-native-uuid": "^2.0.2",
    "react-native-webrtc": "^124.0.0",
    "socket.io-client": "^4.8.1",
    "tslib": "^2.8.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@react-native-community/cli-server-api": "^20.0.2",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.2.79",
    "@types/react-test-renderer": "^18.3.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "detox": "^20.30.1",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.4",
    "react-test-renderer": "19.0.0",
    "typescript": "^5.3.3"
  },
  "expo": {
    "name": "CRYB",
    "slug": "cryb",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.cryb.ios",
      "infoPlist": {
        "NSCameraUsageDescription": "CRYB needs access to your camera for video calls and photo sharing",
        "NSMicrophoneUsageDescription": "CRYB needs access to your microphone for voice calls",
        "NSPhotoLibraryUsageDescription": "CRYB needs access to your photo library to share images"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "app.cryb.android",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-camera",
      "expo-image-picker",
      "expo-notifications",
      "expo-localization"
    ]
  }
}
