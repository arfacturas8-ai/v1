{
  "name": "@cryb/mobile",
  "version": "0.0.1",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:e2e": "detox test",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "expo r -c",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "build:development": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:production": "eas build --profile production",
    "build:ios": "eas build --platform ios --profile production",
    "build:android": "eas build --platform android --profile production",
    "submit:ios": "eas submit --platform ios --profile production",
    "submit:android": "eas submit --platform android --profile production",
    "update": "eas update --branch production",
    "update:preview": "eas update --branch preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/preset-env": "^7.28.3",
    "@cryb/shared": "workspace:*",
    "@expo/metro-runtime": "~3.2.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/cli-server-api": "^20.0.1",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/drawer": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@sentry/react-native": "~6.2.0",
    "@shopify/react-native-skia": "1.7.3",
    "@tanstack/react-query": "^5.64.2",
    "@walletconnect/modal-react-native": "^1.1.0",
    "@walletconnect/react-native-compat": "^2.14.0",
    "expo": "~51.0.0",
    "expo-application": "~6.0.1",
    "expo-av": "~14.0.7",
    "expo-background-fetch": "~12.0.1",
    "expo-battery": "~7.7.2",
    "expo-camera": "~15.0.16",
    "expo-constants": "~17.0.3",
    "expo-crypto": "~14.0.1",
    "expo-device": "~7.0.2",
    "expo-file-system": "~18.0.5",
    "expo-haptics": "~14.0.1",
    "expo-image": "~2.0.1",
    "expo-image-picker": "~16.0.3",
    "expo-keep-awake": "~13.0.2",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.3",
    "expo-local-authentication": "~14.0.1",
    "expo-localization": "~16.0.1",
    "expo-location": "~18.0.3",
    "expo-media-library": "~17.0.4",
    "expo-network": "~6.0.1",
    "expo-notifications": "~0.29.11",
    "expo-secure-store": "~14.0.1",
    "expo-splash-screen": "~0.29.13",
    "expo-status-bar": "~2.0.1",
    "expo-task-manager": "~12.0.1",
    "expo-updates": "~0.26.10",
    "expo-web-browser": "~14.0.1",
    "lottie-react-native": "^7.1.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-error-boundary": "^4.0.13",
    "react-native": "0.76.5",
    "react-native-background-job": "^1.2.4",
    "react-native-biometrics": "^3.0.1",
    "react-native-compressor": "^1.10.2",
    "react-native-device-info": "^13.0.0",
    "react-native-error-boundary": "^1.2.4",
    "react-native-exception-handler": "^2.10.10",
    "react-native-fast-image": "^8.6.3",
    "react-native-flash-message": "^0.4.2",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-gifted-chat": "^2.6.4",
    "react-native-haptic-feedback": "^2.3.3",
    "react-native-keyboard-controller": "^1.18.6",
    "react-native-keychain": "^9.0.0",
    "react-native-mmkv": "^3.2.0",
    "react-native-modal": "14.0.0-rc.1",
    "react-native-netinfo": "^1.1.0",
    "react-native-permissions": "^5.1.0",
    "react-native-reanimated": "~3.16.5",
    "react-native-restart": "^0.0.27",
    "react-native-safe-area-context": "4.14.0",
    "react-native-screens": "~4.4.0",
    "react-native-share": "^11.0.5",
    "react-native-svg": "15.8.0",
    "react-native-uuid": "^2.0.2",
    "react-native-video": "^6.8.1",
    "react-native-voice": "^0.3.0",
    "react-native-web": "~0.19.13",
    "react-native-webrtc": "^124.0.0",
    "socket.io-client": "^4.8.1",
    "viem": "^2.21.58",
    "wagmi": "^2.15.3",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.2",
    "@types/react-native": "~0.73.0",
    "@types/react-test-renderer": "^18.3.0",
    "detox": "^20.30.1",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.2",
    "react-test-renderer": "19.0.0",
    "typescript": "^5.6.3"
  },
  "expo": {
    "name": "CRYB",
    "slug": "cryb",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.cryb.ios",
      "infoPlist": {
        "NSCameraUsageDescription": "CRYB needs access to your camera for video calls and photo sharing",
        "NSMicrophoneUsageDescription": "CRYB needs access to your microphone for voice calls",
        "NSPhotoLibraryUsageDescription": "CRYB needs access to your photo library to share images"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "app.cryb.android",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-camera",
      "expo-image-picker",
      "expo-notifications",
      "expo-localization"
    ]
  }
}
