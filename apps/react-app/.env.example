# Environment
NODE_ENV=development
VITE_API_URL=https://api.cryb.ai/api/v1
VITE_WS_URL=wss://api.cryb.ai

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/cryb_platform
REDIS_URL=redis://localhost:6379

# Authentication
NEXTAUTH_SECRET=your-super-secret-jwt-key-here
NEXTAUTH_URL=http://localhost:3000

# OAuth Providers
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret
DISCORD_CLIENT_ID=your-discord-client-id
DISCORD_CLIENT_SECRET=your-discord-client-secret

# Email Service
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password

# File Storage
VITE_CLOUDINARY_CLOUD_NAME=your-cloudinary-name
CLOUDINARY_API_KEY=your-cloudinary-api-key
CLOUDINARY_API_SECRET=your-cloudinary-api-secret
VITE_RECORDING_BUCKET=cryb-recordings
VITE_S3_ENDPOINT=https://s3.amazonaws.com
VITE_S3_ACCESS_KEY=your-s3-access-key
VITE_S3_SECRET_KEY=your-s3-secret-key
VITE_AWS_REGION=us-east-1
AWS_S3_BUCKET=your-s3-bucket
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1

# Socket.io
VITE_SOCKET_URL=ws://localhost:3001
SOCKET_PORT=3001

# LiveKit (for voice/video)
LIVEKIT_API_KEY=your-livekit-api-key
LIVEKIT_API_SECRET=your-livekit-api-secret
VITE_LIVEKIT_WS_URL=ws://localhost:7880

# Analytics
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX
VITE_ENVIRONMENT=production
VITE_APP_VERSION=1.0.0

# Error Monitoring (Sentry)
VITE_SENTRY_DSN=https://xxxxx@sentry.io/xxxxx

# OAuth Providers (Frontend)
VITE_GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com
VITE_DISCORD_CLIENT_ID=your_discord_client_id
VITE_GITHUB_CLIENT_ID=your_github_client_id

# Web3 & Blockchain
VITE_INFURA_PROJECT_ID=your-infura-project-id
VITE_ALCHEMY_API_KEY=your-alchemy-api-key
VITE_WALLETCONNECT_PROJECT_ID=your-walletconnect-project-id

# CRYB Smart Contract Addresses (replace with actual deployed addresses)
# Ethereum Mainnet
VITE_CRYB_TOKEN_MAINNET=0x0000000000000000000000000000000000000000
VITE_CRYB_NFT_MAINNET=0x0000000000000000000000000000000000000000

# Polygon
VITE_CRYB_TOKEN_POLYGON=0x0000000000000000000000000000000000000000
VITE_CRYB_NFT_POLYGON=0x0000000000000000000000000000000000000000

# Sepolia Testnet
VITE_CRYB_TOKEN_SEPOLIA=0x0000000000000000000000000000000000000000
VITE_CRYB_NFT_SEPOLIA=0x0000000000000000000000000000000000000000

# Crypto Payment Gateways
VITE_TRANSAK_API_KEY=
VITE_MOONPAY_API_KEY=

# Feature Flags
VITE_ENABLE_WEB3=true
VITE_ENABLE_VOICE=true
VITE_ENABLE_ANALYTICS=true

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=60
RATE_LIMIT_WINDOW_MS=60000

# Security
CORS_ORIGIN=http://localhost:3000
ENCRYPTION_KEY=your-32-character-encryption-key

# Development
VITE_DEV_MODE=true
ANALYZE=false