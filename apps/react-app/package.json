{
  "name": "cryb-react",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start": "vite preview",
    "serve": "node server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --collectCoverageFrom='src/**/*.{js,jsx,ts,tsx}' --coverageReporters=html,text,lcov,json-summary",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:visual": "playwright test --project=chromium tests/e2e/visual-regression.spec.js",
    "test:accessibility": "pa11y-ci && playwright test tests/e2e/accessibility.spec.js",
    "test:performance": "artillery run tests/performance/load-test.yml && playwright test tests/e2e/performance.spec.js",
    "test:security": "node tests/security/security-scanner.js && playwright test tests/e2e/security.spec.js",
    "test:api": "jest --testPathPattern=tests/api",
    "test:load": "artillery run tests/performance/load-test.yml",
    "test:smoke": "playwright test tests/e2e/smoke.spec.js",
    "test:regression": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:accessibility && npm run test:performance",
    "test:ci": "npm run test:coverage && npm run test:e2e && npm run test:accessibility",
    "test:mobile": "detox test",
    "test:app-store": "node tests/compliance/app-store-validation.js",
    "playwright:install": "playwright install",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:security": "eslint . --ext .js,.jsx,.ts,.tsx --config .eslintrc.security.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install",
    "audit:accessibility": "node tests/accessibility/audit.js",
    "audit:security": "snyk test && npm audit",
    "audit:performance": "lighthouse --output=html --output-path=./test-results/lighthouse-report.html https://platform.cryb.ai",
    "quality:gate": "node tests/quality/quality-gate.js",
    "coverage:merge": "node tests/utils/merge-coverage.js",
    "coverage:threshold": "node tests/utils/coverage-threshold.js",
    "report:generate": "node tests/utils/test-report-generator.js",
    "metrics:collect": "node tests/utils/metrics-collector.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/themes": "^3.2.1",
    "@sentry/react": "^10.20.0",
    "@sentry/tracing": "^7.120.4",
    "@tanstack/react-query": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@walletconnect/ethereum-provider": "^2.19.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.0",
    "driver.js": "^1.4.0",
    "emoji-picker-react": "^4.13.3",
    "ethers": "^6.15.0",
    "express": "^4.21.2",
    "framer-motion": "^11.0.0",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "isomorphic-dompurify": "^2.29.0",
    "livekit-client": "^2.15.7",
    "lucide-react": "^0.544.0",
    "prop-types": "^15.8.1",
    "puppeteer": "^24.22.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.65.0",
    "react-i18next": "^16.1.4",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.30.2",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^2.6.0",
    "typescript": "^5.0.0",
    "yup": "^1.7.1",
    "zod": "^3.25.76",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@playwright/test": "^1.55.1",
    "@storybook/react": "^8.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^5.0.4",
    "artillery": "^2.0.26",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.2.0",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "detox": "^20.24.2",
    "eslint": "^8.57.0",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "lighthouse": "^12.1.0",
    "node-fetch": "^3.3.2",
    "pa11y": "^8.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.1.0",
    "puppeteer": "^24.22.3",
    "rollup-plugin-visualizer": "^6.0.5",
    "snyk": "^1.1295.0",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "terser": "^5.44.0",
    "vite": "^7.1.11",
    "vite-plugin-pwa": "^1.1.0"
  }
}
