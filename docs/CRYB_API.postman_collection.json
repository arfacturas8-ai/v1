	"auth": {
		"type": "bearer",
		"bearer": [
			{
				"key": "token",
				"value": "{{accessToken}}",
				"type": "string"
			}
		]
	},
	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Auto-refresh token if expired",
					"if (pm.globals.get('tokenExpiry')) {",
					"    const now = new Date().getTime();",
					"    const expiry = new Date(pm.globals.get('tokenExpiry')).getTime();",
					"    ",
					"    if (now >= expiry - 60000) { // Refresh 1 minute before expiry",
					"        console.log('Token expired, refreshing...');",
					"        // This would trigger a refresh token request",
					"    }",
					"}"
				]
			}
		}
	],
