{
  "dashboard": {
    "id": null,
    "title": "Cryb Platform - Comprehensive Monitoring v2",
    "tags": ["cryb", "platform", "comprehensive", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Platform Health Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "cryb_business_platform_health_score",
            "legendFormat": "Health Score",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 70},
                {"color": "green", "value": 90}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Active Users",
        "type": "stat",
        "targets": [
          {
            "expr": "cryb_business_daily_active_users{period=\"1d\"}",
            "legendFormat": "Daily Active",
            "refId": "A"
          },
          {
            "expr": "cryb_business_daily_active_users{period=\"7d\"}",
            "legendFormat": "Weekly Active",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "API Performance",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"cryb-api\"}[5m]))",
            "legendFormat": "95th Percentile",
            "refId": "A"
          },
          {
            "expr": "rate(http_requests_total{job=\"cryb-api\",code=~\"5..\"}[5m]) / rate(http_requests_total{job=\"cryb-api\"}[5m]) * 100",
            "legendFormat": "Error Rate",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 2}
              ]
            },
            "unit": "s"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Socket.io Connections",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(cryb_socketio_connected_clients)",
            "legendFormat": "Connected Clients",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Business KPIs - User Growth",
        "type": "timeseries",
        "targets": [
          {
            "expr": "cryb_business_user_registrations{period=\"1d\"}",
            "legendFormat": "Daily Registrations",
            "refId": "A"
          },
          {
            "expr": "cryb_business_daily_active_users{period=\"1d\"}",
            "legendFormat": "Daily Active Users",
            "refId": "B"
          },
          {
            "expr": "cryb_business_user_retention_rate{period=\"7d\"}",
            "legendFormat": "7-Day Retention %",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Content Creation Metrics",
        "type": "timeseries",
        "targets": [
          {
            "expr": "cryb_business_daily_content_creation{type=\"posts\"}",
            "legendFormat": "Posts Created",
            "refId": "A"
          },
          {
            "expr": "cryb_business_daily_content_creation{type=\"comments\"}",
            "legendFormat": "Comments Created",
            "refId": "B"
          },
          {
            "expr": "cryb_business_daily_messages{type=\"all\"}",
            "legendFormat": "Messages Sent",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "bars",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 80,
              "gradientMode": "none"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "System Resource Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "100 - (avg(rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "CPU Usage %",
            "refId": "A"
          },
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
            "legendFormat": "Memory Usage %",
            "refId": "B"
          },
          {
            "expr": "(1 - (node_filesystem_avail_bytes{fstype!=\"tmpfs\"} / node_filesystem_size_bytes{fstype!=\"tmpfs\"})) * 100",
            "legendFormat": "Disk Usage %",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 90}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "Database Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "pg_stat_database_numbackends",
            "legendFormat": "Active Connections",
            "refId": "A"
          },
          {
            "expr": "rate(pg_stat_database_xact_commit[5m])",
            "legendFormat": "Transactions/sec",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(cryb_db_query_duration_seconds_bucket[5m]))",
            "legendFormat": "95th Query Time",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth"
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 16}
      },
      {
        "id": 9,
        "title": "Queue Health",
        "type": "timeseries",
        "targets": [
          {
            "expr": "cryb_bullmq_queue_size{state=\"waiting\"}",
            "legendFormat": "{{queue}} - Waiting",
            "refId": "A"
          },
          {
            "expr": "cryb_bullmq_queue_size{state=\"active\"}",
            "legendFormat": "{{queue}} - Active",
            "refId": "B"
          },
          {
            "expr": "cryb_bullmq_queue_size{state=\"failed\"}",
            "legendFormat": "{{queue}} - Failed",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "stepAfter",
              "fillOpacity": 20
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 16}
      },
      {
        "id": 10,
        "title": "Real-time Communication Metrics",
        "type": "timeseries",
        "targets": [
          {
            "expr": "cryb_socketio_connected_clients",
            "legendFormat": "Connected - {{namespace}}",
            "refId": "A"
          },
          {
            "expr": "rate(cryb_socketio_messages_total[5m])",
            "legendFormat": "Messages/sec - {{event}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(cryb_socketio_message_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "95th Message Latency",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"}
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 11,
        "title": "Voice/Video Quality",
        "type": "timeseries",
        "targets": [
          {
            "expr": "cryb_business_daily_voice_minutes",
            "legendFormat": "Voice Minutes Today",
            "refId": "A"
          },
          {
            "expr": "cryb_business_voice_call_quality_score",
            "legendFormat": "Quality Score",
            "refId": "B"
          },
          {
            "expr": "rate(cryb_voice_calls_total[5m]) * 60",
            "legendFormat": "Calls/min",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"}
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 12,
        "title": "Error Tracking & Alerts",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=~\"cryb.*\",level=~\"error|critical\"} |= \"error\"",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": false,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32}
      },
      {
        "id": 13,
        "title": "Feature Adoption Rates",
        "type": "piechart",
        "targets": [
          {
            "expr": "cryb_business_feature_adoption_rate",
            "legendFormat": "{{feature}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "percent"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "pie",
          "tooltip": {"mode": "single"},
          "legend": {
            "displayMode": "visible",
            "placement": "right",
            "values": ["percent"]
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 40}
      },
      {
        "id": 14,
        "title": "Cache Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(cryb_cache_hits_total[5m]) / (rate(cryb_cache_hits_total[5m]) + rate(cryb_cache_misses_total[5m])) * 100",
            "legendFormat": "{{cache_type}} Hit Rate %",
            "refId": "A"
          },
          {
            "expr": "redis_memory_used_bytes / redis_memory_max_bytes * 100",
            "legendFormat": "Redis Memory Usage %",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 70},
                {"color": "green", "value": 90}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 40}
      },
      {
        "id": 15,
        "title": "Search Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(cryb_search_queries_total[5m])",
            "legendFormat": "Queries/sec - {{type}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(cryb_search_duration_seconds_bucket[5m]))",
            "legendFormat": "95th Search Time",
            "refId": "B"
          },
          {
            "expr": "elasticsearch_cluster_health_status{color=\"green\"}",
            "legendFormat": "ES Cluster Health",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"}
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 40}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "templating": {
      "list": [
        {
          "name": "namespace",
          "type": "query",
          "query": "label_values(cryb_socketio_connected_clients, namespace)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*"
        },
        {
          "name": "queue",
          "type": "query",
          "query": "label_values(cryb_bullmq_queue_size, queue)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "prometheus",
          "expr": "increase(cryb_deployments_total[1m])",
          "iconColor": "blue",
          "enable": true
        },
        {
          "name": "Alerts",
          "datasource": "prometheus", 
          "expr": "ALERTS{alertstate=\"firing\"}",
          "iconColor": "red",
          "enable": true
        }
      ]
    },
    "refresh": "30s",
    "schemaVersion": 30,
    "version": 2,
    "weekStart": ""
  }
}