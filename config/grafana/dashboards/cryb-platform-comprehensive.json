      {
        "id": 15,
        "title": "Database Performance",
        "type": "graph",
        "gridPos": { "h": 8, "w": 8, "x": 8, "y": 28 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(cryb_db_query_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "A"
          },
          {
            "expr": "rate(cryb_db_slow_queries_total[5m])",
