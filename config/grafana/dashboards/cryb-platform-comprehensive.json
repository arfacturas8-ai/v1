        }
      },
      {
        "id": 16,
        "title": "Error Rates",
        "type": "graph",
        "gridPos": { "h": 8, "w": 8, "x": 16, "y": 28 },
        "targets": [
          {
            "expr": "rate(cryb_errors_total[5m])",
            "legendFormat": "{{level}} - {{service}}",
            "refId": "A"
          },
          {
            "expr": "rate(http_requests_total{status_code=~\"5..\"}[5m])",
            "legendFormat": "HTTP 5xx Errors",
