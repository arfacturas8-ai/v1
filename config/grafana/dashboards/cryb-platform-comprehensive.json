        ],
        "yAxes": [
          {
            "label": "Count",
            "show": true
          },
          {
            "show": true
          }
        ],
        "xAxis": {
          "show": true
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "points": false,
        "pointradius": 2,
        "bars": false,
        "stack": false,
        "percentage": false,
        "legend": {
          "show": true
        }
      },
      {
        "id": 13,
        "title": "Technical Performance",
        "type": "row",
        "gridPos": { "h": 1, "w": 24, "x": 0, "y": 27 },
        "collapsed": false
      },
      {
