        "id": 8,
        "title": "Top Error Messages",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum by (message) (count_over_time({level=\"error\"}[1h])))",
            "refId": "A",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {},
              "renameByName": {
                "Value": "Count",
                "message": "Error Message"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
