    "panels": [
      {
        "id": 1,
        "title": "API Availability SLO",
        "type": "stat",
        "targets": [
          {
            "expr": "(1 - (sum(rate(http_requests_total{job=\"cryb-api\",code=~\"5..\"}[30d])) / sum(rate(http_requests_total{job=\"cryb-api\"}[30d])))) * 100",
            "refId": "A",
            "legendFormat": "30-day Availability"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 99.5
                },
                {
                  "color": "green",
                  "value": 99.9
                }
              ]
            },
            "unit": "percent",
            "min": 99,
            "max": 100
          },
          "overrides": []
        },
        "options": {
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ],
            "fields": ""
          },
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "pluginVersion": "8.5.2",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
